{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "watch",
      "label": "npm: watch",
      "isBackground": true,
      "presentation": {
        "reveal": "never"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": {
        "owner": "vite",
        "pattern": {
          "regexp": ""
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^build started\\.\\.\\.$",
          "endsPattern": "^built in (\\d+ms)"
        }
      }
    }
  ]
}